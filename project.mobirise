pe !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row align-items-center\">\n            <div class=\"\" mbr-class=\"{'col-lg-3 col-6' : showText, 'col-lg-7 col-12' : !showText}\" mbr-if=\"showTitle\">\n                <h4 class=\"subscr__title mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".subscr__title\">SUBSCRIBE TO THE NEWSLETTER</h4>\n            </div>\n            <div class=\"\" mbr-class=\"{'col-lg-4 col-6' : showTitle, 'col-lg-7 col-12' : !showTitle}\" mbr-if=\"showText\">\n                <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Repudiandae, quis!</p>\n            </div>\n            <div class=\"\" mbr-class=\"{'col-lg-5 col-12' : showTitle &amp;&amp; showText, 'col-lg-12 col-12' : !showTitle &amp;&amp; !showText}\">\n                <div mbr-form=\"\" class=\"subscribe__form\">\n                    <!---Formbuilder Form--->\n                    <form action=\"\" name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                        <div class=\"row\">\n                            <div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">Thanks for filling out the form!</div>\n                            <div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\">\n                            </div>\n                        </div>\n                        <div class=\"dragArea row\">\n                            <div class=\"form-group col-md-9 col-lg-7 col-8 \" data-for=\"email\">\n                                <input type=\"email\" name=\"email\" placeholder=\"Your Email\" data-form-field=\"Email\" mbr-theme-style=\"display-4\" required=\"required\" class=\"form-control\">\n                            </div>\n                            <div class=\"col-md-3 col-lg-5 col-4 input-group-btn \" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd,-mbrBtnRemove,-mbrLink\"><a type=\"submit\" class=\"btn btn-lg btn-primary\" data-app-placeholder=\"Type Text\">Subscribe</a></div>\n                        </div>\n                    </form><!---Formbuilder Form--->\n                </div>\n        </div>\n        </div>\n    </div>\n</section>",
